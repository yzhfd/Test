{% extends '::base.xml.twig' %}

{% block body %}
    <App>
        {% for magzine in magzines %}
        <Magazine>
            <Id>{{ magzine.id }}</Id>
            <Name>{{ magzine.name }}</Name>
            <Cover>{% if magzine.landscapeCover is not null %}{{ magzine.landscapeCover }}{% endif %}</Cover>
            <Url>{{ url('output_magzine', { 'id':magzine.id }) }}</Url>
        </Magazine>
        {% endfor %}
    </App>
{% endblock %}