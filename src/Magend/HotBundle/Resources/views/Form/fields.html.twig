{# collection is parent #}
{% block assets_widget %}
    {% spaceless %}
        {% if prototype is defined %}
            {% set attr = attr|merge({'data-prototype': form_row(prototype) }) %}
        {% endif %}
        
        {% if multiple %}{{ name }}{% endif %}
        <div class="assets" style="width:300px; height:200px; background:#333;"></div>
        {{ block('form_widget') }}
    {% endspaceless %}
{% endblock %}